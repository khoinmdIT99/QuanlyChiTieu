@{
    ViewBag.Title = "Not Found";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Main content -->
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-red">404</h2>

            <div class="error-content">
                <h3><i class="fa fa-warning text-red"></i> Resource Not Found</h3>

                <p><strong>Message:</strong> @ViewData["ErrorMessage"]</p>

                <a href="@Url.Action("Index", "Dashboard")" class="btn btn-default btn-flat" role="button">Home</a>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
